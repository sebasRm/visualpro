<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constrain_domicilio"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".interfaz.PresentacionActivity">

    <androidx.cardview.widget.CardView
        android:layout_width="485dp"
        android:layout_height="168dp"
        android:layout_marginTop="8dp"
        android:background="#AE970202"
        app:cardBackgroundColor="#05FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#DAD1D1D3">

            <TextView
                android:id="@+id/lblCTelefonoCliente"
                android:layout_width="116dp"
                android:layout_height="28dp"
                android:layout_marginTop="32dp"
                android:text="TextView"
                android:textColor="#0E0E0E"
                android:textSize="18sp"
                app:layout_constraintStart_toEndOf="@+id/imageView37"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/lblReferenciaCliente"
                android:layout_width="192dp"
                android:layout_height="26dp"
                android:layout_marginStart="64dp"
                android:layout_marginTop="4dp"
                android:text="TextView"
                android:textColor="#070707"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblCDireccionCliente" />

            <TextView
                android:id="@+id/lblCNombreCliente"
                android:layout_width="198dp"
                android:layout_height="28dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="8dp"
                android:text="TextView"
                android:textColor="#090909"
                android:textSize="18sp"
                app:layout_constraintStart_toEndOf="@+id/imageView30"
                app:layout_constraintTop_toBottomOf="@+id/lblCidentificacion" />

            <TextView
                android:id="@+id/lblCApellidoCliente"
                android:layout_width="107dp"
                android:layout_height="25dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="4dp"
                android:text="TextView"
                android:textColor="#0B0A0A"
                android:textSize="18sp"
                android:visibility="gone"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblCNombreCliente" />

            <TextView
                android:id="@+id/lblCDireccionCliente"
                android:layout_width="194dp"
                android:layout_height="36dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="16dp"
                android:text="TextView"
                android:textColor="#020202"
                android:textSize="18sp"
                app:layout_constraintStart_toEndOf="@+id/imageView31"
                app:layout_constraintTop_toBottomOf="@+id/lblCNombreCliente" />

            <TextView
                android:id="@+id/lblCidentificacion"
                android:layout_width="201dp"
                android:layout_height="32dp"
                android:layout_marginTop="12dp"
                android:text="TextView"
                android:textColor="#040303"
                android:textSize="24sp"
                app:layout_constraintStart_toEndOf="@+id/imageView32"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/lblTiempo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="#0E0E0E"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView30"
                android:layout_width="41dp"
                android:layout_height="34dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="12dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView32"
                app:srcCompat="@mipmap/dcliente" />

            <ImageView
                android:id="@+id/imageView31"
                android:layout_width="42dp"
                android:layout_height="43dp"
                android:layout_marginStart="16dp"
                android:scaleType="centerCrop"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView30"
                app:srcCompat="@mipmap/ddireccion" />

            <ImageView
                android:id="@+id/imageView32"
                android:layout_width="40dp"
                android:layout_height="32dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="12dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@mipmap/tel" />

            <ImageButton
                android:id="@+id/btnDomicilioModificar"
                android:layout_width="59dp"
                android:layout_height="60dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:background="#00FFFFFF"
                android:scaleType="centerCrop"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/btnEliminarPedidoCliente"
                app:srcCompat="@mipmap/editar" />

            <ImageButton
                android:id="@+id/btnEliminarPedidoCliente"
                android:layout_width="62dp"
                android:layout_height="64dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="8dp"
                android:background="#00FFFFFF"
                android:scaleType="centerCrop"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@mipmap/eliminar" />

            <ImageView
                android:id="@+id/imageView37"
                android:layout_width="50dp"
                android:layout_height="47dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="32dp"
                android:scaleType="centerCrop"
                app:layout_constraintStart_toEndOf="@+id/lblCNombreCliente"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@mipmap/precio" />


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>